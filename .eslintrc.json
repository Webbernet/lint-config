{
    "extends": ["plugin:vue/essential", "eslint:recommended"],
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true,
            "impliedStrict": true
        }
    },
    "rules": {
      "brace-style": [2, "1tbs", { "allowSingleLine": true }],
      "comma-dangle": [2, "never"],
      "complexity": [2, 6],
      "curly": 2,
      "eqeqeq": [2, "allow-null"],
      "max-statements": [2, 30],
      "no-shadow-restricted-names": 2,
      "no-undef": 2,
      "no-use-before-define": 2,
      "radix": 2,
      "semi": 2,
      "strict": 2
    },
    "globals": {
      "Routes": true,
      "axios": true,
      "moment": true
    }
}
